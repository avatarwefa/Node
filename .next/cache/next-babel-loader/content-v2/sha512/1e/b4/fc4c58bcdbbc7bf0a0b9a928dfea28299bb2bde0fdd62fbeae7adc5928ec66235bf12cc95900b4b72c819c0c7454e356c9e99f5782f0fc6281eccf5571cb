\n        setDisLikeCount(dislikeCount += 1);\r\n    }\r\n    function toggleLike(){\r\n        setLiked(!liked);\r\n    }\r\n    function toggleDisLike(){\r\n        setDisLiked(!disliked);\r\n    }\r\n    function toggleReview() {\r\n        setShowReview(showReview = true);\r\n        setShowComment(showComment = false);\r\n    }\r\n    function toggleComment() {\r\n        setShowReview(showReview = false);\r\n        setShowComment(showComment = true);\r\n    }\r\n    return (\r\n        <Layout showFooter={true}>\r\n            <div className=\"buster-light\">\r\n                <div className=\"hero mv-single-hero\">\r\n                    <div className=\"container\">\r\n                        <div className=\"row\">\r\n                            <div className=\"col-md-12\">\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div className=\"page-single movie-single movie_single\">\r\n                    <div className=\"container\">\r\n                        <div className=\"row ipad-width2\">\r\n                            <div className=\"col-md-4 col-sm-12 col-xs-12\">\r\n                                <div className=\"movie-img sticky-sb\">\r\n                                    <img src=\"images/uploads/movie-single.jpg\" alt=\"\" />\r\n                                    <div className=\"movie-btn\">\r\n                                        <div className=\"btn-transform transform-vertical red\">\r\n                                            <div><a href=\"#\" className=\"item item-1 redbtn\"> <i className=\"ion-play\"></i> Watch Movie</a></div>\r\n                                            <div><a href=\"https://www.youtube.com/embed/o-0hcF97wy0\" className=\"item item-2 redbtn fancybox-media hvr-grow\"><i className=\"ion-play\"></i></a></div>\r\n                                        </div>\r\n                                        <div classNa